{
    "routes": [
        {
            "match": "^/plaintext/?.*$",
            "rewrite": {
                "src": "^/plaintext(/?.*)$",
                "dest": "$1"
            },
            "destination": "http://localhost:5001"
        },
        {
            "match": "^/json/?.*$",
            "rewrite": {
                "src": "^/json(/?.*)$",
                "dest": "$1"
            },
            "destination": "http://localhost:5002"
        }
    ]
}